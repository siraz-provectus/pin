<% if user_signed_in? %>
  <%#= render 'shared/signed_user'%>
<% else %>
  <%= render 'shared/unsigned_user'%>
<% end %>
<div id="newPinModal" class="modal fade">
</div>
<div>
	<h3> Все новости </h3>
</div>

<div id='paginator'>
  <%#= paginate @pins, params: { controller: 'pins', action: 'load' },
    param_name: :pins_page, remote: true %>
</div>

<div id='allpins'>
	<%= render 'pins/pins', pins: @pins %>
</div>

<nav id="page-nav">
  <%= link_to '',load_pins_url(pins_page:2)%>
</nav>

<%= content_for :javascript_code do %>
    MasonaryLoad.load();
    CommentsManagement.initialize();
<% end %>